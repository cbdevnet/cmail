#!/usr/bin/expect -f

set mailfrom "" 
set mailto "postmaster"
set subject "Special mailpaths test"

source ../primitives/common
source ../primitives/smtp_init
run reset_database
run cmail-admin-address add "postmaster%" drop

set spawn_id [smtp_init]

send "MAIL FROM:<$mailfrom>\r"
expect "250 "
send "RCPT TO:<$mailto>\r"
expect "250 "

source ../primitives/smtp_data

send_user "Done.\n"
